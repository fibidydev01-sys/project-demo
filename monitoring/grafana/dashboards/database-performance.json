{
  "id": null,
  "uid": "firma-database",
  "title": "Firma Hukum PERARI - Database Performance",
  "tags": ["firma", "database", "prisma", "postgresql"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Query Rate (QPS)",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(db_queries_total[1m]))",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "qps",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 50, "color": "yellow" },
              { "value": 100, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Avg Query Time",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(db_query_duration_seconds_bucket[5m])) * 1000",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 500, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Slow Queries (>1s)",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(db_slow_queries_total[5m]))",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "qps",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 5, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Active Connections",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "db_active_connections",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 8, "color": "yellow" },
              { "value": 15, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Query Rate by Operation",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum by(operation) (rate(db_queries_total[5m]))",
          "legendFormat": "{{operation}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "qps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Query Duration Percentiles",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(db_query_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "p50",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        },
        {
          "expr": "histogram_quantile(0.95, rate(db_query_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "p95",
          "refId": "B",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        },
        {
          "expr": "histogram_quantile(0.99, rate(db_query_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "p99",
          "refId": "C",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Slow Queries Over Time",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum by(model) (rate(db_slow_queries_total[5m]))",
          "legendFormat": "{{model}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Database Errors",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum by(error_type) (rate(db_errors_total[5m]))",
          "legendFormat": "{{error_type}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    },
    {
      "id": 9,
      "type": "table",
      "title": "Top 10 Slowest Models (P95)",
      "gridPos": { "x": 0, "y": 20, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "topk(10, histogram_quantile(0.95, sum by(model, le) (rate(db_query_duration_seconds_bucket[5m]))) * 1000)",
          "refId": "A",
          "format": "table",
          "instant": true,
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "le": true },
            "renameByName": {
              "model": "Model",
              "Value": "P95 Latency (ms)"
            }
          }
        }
      ]
    },
    {
      "id": 10,
      "type": "table",
      "title": "Query Count by Model",
      "gridPos": { "x": 12, "y": 20, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "topk(10, sum by(model) (rate(db_queries_total[5m])))",
          "refId": "A",
          "format": "table",
          "instant": true,
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": {
              "model": "Model",
              "Value": "Queries/sec"
            }
          }
        }
      ]
    }
  ]
}
